type UserRole = 
 variant {
   Client;
   ServiceProvider;
 };
type Time = int;
type Result_1 = 
 variant {
   err: text;
   ok: vec Profile;
 };
type Result = 
 variant {
   err: text;
   ok: Profile;
 };
type ProfileImage = 
 record {
   imageUrl: text;
   thumbnailUrl: text;
 };
type Profile = 
 record {
   biography: opt text;
   createdAt: Time;
   email: text;
   id: principal;
   isVerified: bool;
   name: text;
   phone: text;
   profilePicture: opt ProfileImage;
   role: UserRole;
   updatedAt: Time;
 };
service : {
  getAllProfiles: () -> (vec Profile) query;
  getAllServiceProviders: () -> (vec Profile) query;
  getProfile: (userId: principal) -> (Result) query;
  testCreateProfile: () -> (Result);
  testGetAllServiceProviders: () -> (Result_1);
  testInvalidEmail: () -> (Result);
  testInvalidPhone: () -> (Result);
  testUpdateProfile: () -> (Result);
  testVerifyUser: () -> (Result);
}
