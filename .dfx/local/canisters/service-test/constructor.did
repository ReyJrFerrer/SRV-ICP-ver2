type Time = int;
type ServiceStatus = 
 variant {
   Available;
   Suspended;
   Unavailable;
 };
type ServiceCategory = 
 record {
   description: text;
   id: text;
   name: text;
   parentId: opt text;
   slug: text;
 };
type Service = 
 record {
   category: ServiceCategory;
   createdAt: Time;
   description: text;
   id: text;
   location: Location;
   price: nat;
   providerId: principal;
   rating: opt float64;
   reviewCount: nat;
   status: ServiceStatus;
   title: text;
   updatedAt: Time;
 };
type Result_1 = 
 variant {
   err: text;
   ok: vec Service;
 };
type Result = 
 variant {
   err: text;
   ok: Service;
 };
type Location = 
 record {
   address: text;
   city: text;
   country: text;
   latitude: float64;
   longitude: float64;
   postalCode: text;
   state: text;
 };
service : {
  getAllCategories: () -> (vec ServiceCategory) query;
  getAllServices: () -> (vec Service) query;
  getService: (serviceId: text) -> (Result) query;
  testCreateService: () -> (Result);
  testGetServiceByProvider: () -> (Result_1);
  testSearchServicesByLocation: () -> (Result_1);
  testUpdateServiceRating: () -> (Result);
  testUpdateServiceStatus: () -> (Result);
}
